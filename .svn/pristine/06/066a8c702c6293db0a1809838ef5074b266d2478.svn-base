@charset "UTF-8";

@main-color: #0096e0;
@main-color-hover: #038cd0;

.fl {float: left}
.fr {float: right}
.pos-r {position: relative}
.wrap-bd {padding: 20px}
.mt-default {margin-top: 18px}
.mr-default {margin-right: 18px}
.mb-default {margin-bottom: 18px}
.ml-default {margin-left: 18px}
.mt-small {margin-top: 5px}
.mr-small {margin-right: 5px}
.mb-small {margin-bottom: 5px}
.ml-small {margin-left: 5px}
.hidden {display: none}
.dis-ib{display: inline-block}
.ta-l {text-align: left}
.va-t {vertical-align: top}
.cur-p {cursor: pointer}
.box-sz {box-sizing: border-box;}
.bg {background: #fff;}
.clearfix {
    display: block;
    zoom: 1;
    &:after {
        content: "";
        display: block;
        height: 0;
        clear: both;
        visibility: hidden
    }
}
* {
    color: #666
}
body {
    font-family: "Microsoft YaHei", sans-serif;
    background: #e7e8eb
}
a {
    color: #007fbf
}
h1,
h2,
h3,
h4,
h5,
h6 {
    color: #333
}
img {vertical-align: middle}
input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #aaa !important;
}
input:-moz-placeholder,
textarea:-moz-placeholder {
    color: #aaa !important; /* Mozilla Firefox 4 to 18 */
}
input::-moz-placeholder,
textarea::-moz-placeholder {
    color: #aaa !important; /* Mozilla Firefox 19+ */
}
input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #aaa !important; /* Internet Explorer 10+ */
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
/* text body字体颜色为#666 */
.txt-warn {color: #fa5829}
.txt-success {color: #06ada3}
.txt-info {color: #333}
.txt-log {color: #ccc}
/* table */
.op-section {
    margin-bottom: 1px;
    padding: 15px 25px;
    background: #fff;
}
/* table */
.table-contain {
    width: 100%;
    overflow: auto;
    pre {
        margin: 0;
        font-size: 0;
        white-space: nowrap;
    }
}
.table-border {
    border: 1px solid #dad9d9
}
.table-list {
    margin-left: auto;
    margin-right: auto;
    width: 100%;
    text-align: center;
    tr{
        th,
        td {
            padding: 15px 10px;
            border: 1px solid #dad9d9;
            line-height: 20px;
            font-size: 14px;
            word-break: break-all;
            font-weight: normal;
            &:first-child {
                border-left: 0 none;
            }
            &:last-child {
                border-right: 0 none;
            }
        }
        th {
            color: #333
        }
    }
    .table-thead {
        background-color: #f4f5f8
    }
    .table-tbody {
        background-color: #fff;
        tr {
            &:hover {
                background: #f9fafb
            }
        }
    }
    .table-op {
        a {
            text-decoration: none;
            color: #666;
            &:hover {
                color: #333;
            }
        }
    }
}
/* label */
.label {
    width: 100px;
    display: inline-block;
    line-height: 34px;
}
/* input */
.input {
    box-sizing: border-box;
    padding: 0 8px;
    height: 34px;
    border: 1px solid #ccc;
    font-size: 14px;
    font-weight: normal;
    vertical-align: middle;
    &.input-default {
        width: 200px
    }
    &.input-full {
        width: 100%
    }
    &.input-date {
        padding-right: 28px;
        width: 130px;
        background: url(../images/ipt-date.png) no-repeat 100px center;
    }
    &.input-search-text {
        padding-left: 28px;
        width: 250px;
        background: url(../images/ipt-search.png) no-repeat 8px center;
    }
    &.input-search-button {
        border: 1px solid @main-color;
        padding: 0 20px;
        background: @main-color;
        font-size: 16px;
        color: #fff;
        cursor: pointer;
        &:hover {
            background: @main-color-hover;
        }
    }
    &.input-search-button-white {
        padding: 0 20px;
        background: linear-gradient(#fdfdfd, #f7f7f7);
        font-size: 16px;
        color: #333;
        cursor: pointer;
        &:hover {
            background: linear-gradient(#fafafb, #f1f2f5)
        }
    }
}
.checkbox,
.radio {
    vertical-align: middle;
    margin-top: -2px;
    margin-bottom: 1px;
}
/* select */
.select {
    box-sizing: border-box;
    padding: 0 0 0 5px;
    height: 34px;
    border: 1px solid #ccc;
    vertical-align: middle;
    font-size: 14px;
    option {
        padding: 0 6px;
    }
}
/* textarea */
.textarea {
    box-sizing: border-box;
    padding: 8px;
    border: 1px solid #ccc;
    resize: vertical;
    font-size: 14px;
    vertical-align: top;
    &.textarea-default {
        width: 200px
    }
    &.textarea-full {
        width: 100%
    }
}
/* required */
.required {
    &:after{
        content: '*';
        color: #ed4b1c;
    }
}
/* pills */
.pills {
    height: 34px;
    padding: 0 10px;
    line-height: 34px;
    display: inline-block;
    font-size: 14px;
    color: #666;
    cursor: pointer;
    text-decoration: none;
    vertical-align: middle;
    &:hover {
        background: #f4f5f8;
        color: #333
    }
    &.pills-active {
        background: @main-color !important;
        color: #fff !important
    }
}
/* crumb */
.crumb {
    display: inline-block;
    font-size: 14px;
    color: #0096e0;
    text-decoration: none;
    &:after {
        padding: 0 5px;
        color: @main-color;
        content: '>'
    }
    &.crumb-active {
        color: @main-color;
        &:after {
            content: ''
        }
    }
}
/* badge */
.badge {
    display: inline-block;
    min-width: 10px;
    padding: 4px 6px;
    font-size: 12px;
    font-style: normal;
    line-height: 1;
    color: #fff;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    background: @main-color;
    border-radius: 9px;
    z-index: 22;
}
/* button */
.button {
    box-sizing: border-box;
    padding: 5px 8px;
    line-height: normal;
    display: inline-block;
    font-size: 16px;
    font-weight: 400;
    text-align: center;
    text-decoration: none;
    vertical-align: middle;
    cursor: pointer;
    &.button-default {
        border: 1px solid @main-color;
        background: @main-color;
        color: #fff;
        &:hover {
            background: @main-color-hover
        }
    }
    &.button-white {
        border:1px solid #ccc;
        color: #333;
        background: linear-gradient(#fdfdfd, #f7f7f7);
        &:hover {
            background: linear-gradient(#fafafb, #f1f2f5)
        }
    }
    &.button-ok {
        border: 1px solid #0096e0;
        min-width: 100px;
        height: 38px;
        background: @main-color;
        color: #fff;
        font-size:18px;
        &:hover {
            background: @main-color-hover
        }
    }
    &.button-cancel {
        border:1px solid #ccc;
        min-width:100px;
        height:38px;
        font-size:18px;
        color: #333;
        background: linear-gradient(#fdfdfd, #f7f7f7);
        &:hover {
            background: linear-gradient(#fafafb, #f1f2f5)
        }
    }
    &.button-full {
        width: 100%;
    }
}
/* icon */
.icon-op {
    position: relative;
    top: 2px;
    width: 16px;
    height: 16px;
    display: inline-block;
    background: url(../images/icon-operate.png) no-repeat;
    cursor: pointer;
    &.icon-op-edit {
        background-position: 0 0;
    }
    &.icon-op-del {
        background-position: -17px 0;
    }
    &.icon-op-copy {
        background-position: -34px 0;
    }
}
/* dialog */
.dialog {
    position: fixed;
    min-width: 260px;
    background: #fff;
    box-shadow: 0 0 100px #999;
    z-index: 9999;
    .dialog-padding {
        padding-left: 100px !important;
        padding-right:100px !important;
    }
    .dialog-head {
        padding: 15px;
        height: 20px;
        line-height: 20px;
        background: #53585f;
        font-size: 16px;
        color: #fff;
        overflow: hidden
    }
    .dialog-body {
        padding: 20px 30px;
        background: #fff;
        .limit-height {
            max-height: 568px;
            overflow: auto;
        };
    }
    .dialog-foot {
        padding: 0 15px 15px;
        text-align: center;
        background: #fff;
        overflow: hidden
    }
    .dialog-button {
        padding: 8px 16px;
        display: inline-block;
        font-size: 16px;
    }
    .dialog-ok {
        border:1px solid @main-color;
        background: @main-color;
        color: #fff;
        &:hover {
            background: @main-color-hover
        }
    }
    .dialog-cancel {
        border:1px solid #ccc;
        background: linear-gradient(#fdfdfd, #f7f7f7);
        color: #333;
        &:hover {
            background: linear-gradient(#fafafb, #f1f2f5)
        }
    }
}
/* cover */
.cover-all {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: none;
    background: #000;
    opacity: 0.4;
    z-index: 999
}
.cover-loading {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000 url(../images/loading.gif) no-repeat center;
    opacity: 0.4;
    z-index: 999;
}
/* data */
.data-empty {
    padding: 108px 0 !important;
    background: #fff url(../images/data-empty.png) no-repeat center;
}
.data-conflict {
    padding: 108px 0 !important;
    background: #fff url(../images/data-conflict.png) no-repeat center;
}
/* print */
@media print {
    *,
    *:before,
    *:after,
    *:first-letter,
    *:first-line {
        background: transparent !important;
        color: #000 !important;
        box-shadow: none !important;
        text-shadow: none !important;
    }
    a,
    a:visited {
        text-decoration: underline;
    }
    .no-print {
        display: none
    }
}
